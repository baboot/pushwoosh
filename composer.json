{
    "name": "baboot/pushwoosh",
    "description": "A Pushwoosh bridge for Laravel",
    "license": "MIT",
    "keywords": ["pushwoosh", "push", "notification", "hoy", "laravel"],
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "vincent@hoy.se"
        }
    ],
    "require": {
        "php": "^5.6.4 || ^7.0",
        "illuminate/contracts": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.*",
        "illuminate/support": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.*",
        "graham-campbell/manager": "^4.0",
        "gomoob/php-pushwoosh": "^1.9"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.2",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Baboot\\Pushwoosh\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Baboot\\Tests\\Pushwoosh\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
